    sleep 1
    play :A5, sustain: 1
    sleep 1
    #Measure 4
    play :Fs5, sustain: 2
    play :D5, sustain: 2
    play :A4, sustain: 2
    sleep 4
    play :Fs5, sustain: 1
    play :D5, sustain: 1
    play :A4, sustain: 1
    sleep 1
    play :Gs5, sustain: 1
    play :B4, sustain: 1
    sleep 1
    play :E5, sustain: 1
    play :B4, sustain: 1
    sleep 1
    play :D5, sustain: 1
    play :B4, sustain: 1
    sleep 1
    #Measure 5
    play :E5, sustain: 2
    play :B4, sustain: 2
    play :G4, sustain: 2
    sleep 4
    play :E5, sustain: 1
    play :B4, sustain: 1
    play :G4, sustain: 1
    sleep 1
    play :Fs5, sustain: 1
    play :A4, sustain: 1
    sleep 1
    play :D5, sustain: 1
    play :A4, sustain: 1
    sleep 1
    play :Cs5, sustain: 1
    play :A4, sustain: 1
    sleep 1
    #Measure 6
    play :B4, sustain: 1
    play :D5, sustain: 1
    sleep 1
    play :E5, sustain: 1
    play :B4, sustain: 1
    sleep 1
    play :Fs5, sustain: 1
    play :B4, sustain: 1
    sleep 2#1
    play :Fs5, sustain: 1
    play :B4, sustain: 1
    sleep 1
    play :Gs5, sustain: 1
    sleep 1
    play :A5, sustain: 1
    sleep 1
    play :B5, sustain: 1
    sleep 1
    #Measure 7
    play :A5, sustain: 2
    sleep 4 #5
    play :Gs5, sustain: 1.5
    play :Es5, sustain: 1.5
    sleep 2 #3
    sleep 2
  end
  stop
end
