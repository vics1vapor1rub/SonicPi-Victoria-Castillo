    sleep 1
    play :Cs6, sustain: 1
    sleep 1
    #Measure 3
    play :Gs5, sustain: 1.5
    play :Cs5, sustain: 1.5
    play :Es5, sustain: 1.5
    sleep 3#2
    play :A5, sustain: 0.5
    sleep 1
