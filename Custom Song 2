use_bpm 98
use_synth :chiplead
live_loop :si do #48 beats
  with_fx :reverb do
    #Treble Clef#
    #Measure 1#
    play :Fs5, sustain: 1
    sleep 1
    play :Cs6, sustain: 1
    sleep 1
    #Measure 2#
    play :Gs5, sustain: 1.5
    play :Cs5, sustain: 1.5
    play :Es5, sustain: 1.5
    sleep 3
    play :A5, sustain: 0.5
    sleep 1
